<configuration>
  <system.serviceModel>
    <services>
      <service name="Microsoft.Samples.MSMQPoison.OrderProcessorService">
        <!-- Define NetMsmqEndpoint -->
        <endpoint address="net.msmq://localhost/private/ServiceModelSamplesPoison;poison" binding="netMsmqBinding" bindingConfiguration="netMsmqBindingConfig" contract="Microsoft.Samples.MSMQPoison.IOrderProcessor"></endpoint>
      </service>
    </services>
    <bindings>
      <netMsmqBinding>
        <binding name="netMsmqBindingConfig">
          <security mode="None" />
        </binding>
      </netMsmqBinding>
    </bindings>
  </system.serviceModel>
</configuration>
